<Window x:Class="Modeler.DialogBoxes.NameDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Nowa nazwa" Height="165" Width="432" Icon="/Modeler;component/Ikony/logo3d20x20.png" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Image Name="Image" DockPanel.Dock="Left" />
        <TextBox Height="23" Name="Name" Width="242" Margin="156,64,0,0" KeyUp="KeyUpEv" VerticalAlignment="Top" HorizontalAlignment="Left" />
        <Button Content="Anuluj" Height="23" Name="CancelButton" Margin="323,91,0,0" Click="CancelButton_Click" VerticalAlignment="Top" HorizontalAlignment="Left" Width="75" />
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="242,91,0,0" Name="OkButton" VerticalAlignment="Top" Width="75" 
                IsDefault="True" Click="OkButton_Click" />
        <Label Content="Nowa nazwa:" Height="28" HorizontalAlignment="Left" Margin="12,64,0,0" Name="label1" VerticalAlignment="Top" Width="105" />
        <TextBox Height="46" HorizontalAlignment="Left" Margin="12,12,0,0" Name="Message" VerticalAlignment="Top" Width="386" TextAlignment="Center"
                 BorderThickness="0"/>
        
    </Grid>
</Window>
